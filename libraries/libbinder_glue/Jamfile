# Jamfile to build libbinder_glue library
PSSubDir TOP libraries libbinder_glue ;

# Build the static lib
Library libbinder_glue : binder_glue.cpp ;

MakeLocate libbinder_glue$(SUFLIB) : $(LIB_DIR) ;

